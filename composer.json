{
    "name": "luminix/bi",
    "description": "The Luminix extension to create beautiful dashboards with Laravel",
    "license": "MIT",
    "scripts": {
        "test": "@php vendor/bin/testbench package:test"
    },
    "authors": [
        {
            "name": "Alberto Bottarini",
            "email": "alberto.bottarini@gmail.com"
        },
        {
            "name": "Bruno Polo",
            "email": "brunopolo@poli.ufrj.br"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~11.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Luminix\\Bi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Luminix\\Bi\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Luminix\\Bi\\BiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
